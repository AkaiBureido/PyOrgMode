<div class="org-src-container"><label class="org-src-name">class_Property()</label><pre class="src src-python">
<span class="org-keyword">class</span> <span class="org-type">Property</span>(OrgElement):
    <span class="org-string">"""
    A Property object, used in drawers.
    """</span>
    <span class="org-keyword">def</span> <span class="org-function-name">__init__</span>(<span class="org-keyword">self</span>,name=<span class="org-string">""</span>,value=<span class="org-string">""</span>):
        OrgElement.__init__(<span class="org-keyword">self</span>)
        <span class="org-keyword">self</span>.name = name
        <span class="org-keyword">self</span>.value = value
    <span class="org-keyword">def</span> <span class="org-function-name">__str__</span>(<span class="org-keyword">self</span>):
        <span class="org-string">"""
        Outputs the property in text format (e.g. :name: value)
        """</span>
        <span class="org-keyword">return</span> <span class="org-string">":"</span> + <span class="org-keyword">self</span>.name + <span class="org-string">": "</span> + <span class="org-keyword">self</span>.value
</pre></div>


</div>

